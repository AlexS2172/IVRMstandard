	<script language='VBScript'><![CDATA[Sub CreateShortcut(target, sname)

	Set objWshShell = CreateObject("Wscript.Shell")
	
	' Read desktop path using WshSpecialFolders object
	strDesktopPath = objWshShell.SpecialFolders("Desktop")
	
	' Create a shortcut object on the desktop
	Set objShortcut = objWshShell.CreateShortcut(strDesktopPath & "\" & sname & ".lnk")
	
	' Set shortcut object properties and save it
	objShortcut.TargetPath = target
	objShortcut.Save
End Sub

Public Function UpdateVersion(ByVal vstrVersion)
	Dim larrVer
	larrVer       = Split(vstrVersion, ".")
	larrVer(2)    = CLng(larrVer(2)) + 1
	UpdateVersion = Join(larrVer, ".")
End Function

Public Sub IncrementISVersion(ByVal strFileName)
	Dim  ISWiProject
    Dim  pProperty 
	Builder.logmessage strFileName
	' Create a reference to the InstallShield Developer Automation Interface
  	Set ISWiProject = CreateObject("ISWiAutomation.ISWiProject")
  	    ISWiProject.OpenProject(strFileName)
  	    
    Set pProperty = ISWiProject.ISWIProperties.Item("ProductVersion")

	Builder.logmessage "The ISProject " + strFileName +" Version " + pProperty.Value
	pProperty.Value = UpdateVersion(pProperty.Value)
	Builder.logmessage "Is changed to " +pProperty.Value     
   
    ISWiProject.SaveProject
    ISWiProject.CloseProject  	
End Sub

' *****************************************************************************
' get  trapping any errors


Function PadLeft(str, size)
	If size > Len(str) Then
		PadLeft = String(size-Len(str), 32) & str
	Else
		PadLeft = str
	End If
End Function

Function PadRight(str, size)
	If size > Len(str) Then
		PadRight = str & String(size-Len(str), 32)
	Else
		PadRight = str
	End If
End Function

Public Function GetFileVersions(ByVal vstrPath)

	Dim strResult, strVersion
	Dim fso
	Dim objFolder, dateStr, dateArr
	
	
	strResult = " <table border=0 cellspacing=0 cellpadding=0 > <tr> <td width=190 valign=top><b>File</b></td><td width=70 valign=top ><b>Size</b></td><td width=100 valign=top ><b>Version</b></td><td width=250 valign=top><b>Modification</b> </td> </tr>"
	Set fso       = vbld_FSO
	Set objFolder = fso.GetFolder(vstrPath)
	For Each objFile In objFolder.Files
	  If UCase(Right(objFile.Name, 3)) = "DLL" Or UCase(Right(objFile.Name, 3)) = "EXE" Or UCase(Right(objFile.Name, 3)) = "OCX" Then

			dateStr = Trim(objFile.DateLastModified)
			dateArr = Split(dateStr, " ")
			dateArr(1) = Trim(dateArr(1))
			If Len(dateArr(1)) < 8 Then dateArr(1) = "0" & dateArr(1)
			dateStr = Join(dateArr, " ")

			strResult = strResult & "<tr><td width=190 valign=top><span lang=EN-US style='font-size:8.0pt;'>"
			strResult = strResult & objFile.Name
			strResult = strResult & "</span></td><td width=70 valign=top><span lang=EN-US style='font-size:8.0pt;'>"
			strResult = strResult & FormatNumber(objFile.Size, 0, False, False, True)
			strResult = strResult & "</span></td><td width=100 valign=top><span lang=EN-US style='font-size:8.0pt;'>"
			strResult = strResult & fso.GetFileVersion(objFile.Path)
            strResult = strResult & "</span></td><td width=250 valign=top><span lang=EN-US style='font-size:8.0pt;'>"
            strResult = strResult & dateStr
            strResult = strResult & "</span></td></tr>"
	  End If 
	Next	
	GetFileVersions = strResult+ "</table>"
End Function

Public Function GetDBVersionFromFileName(ByVal vstrFileName)
'20040112.02_ViaNet_3.33.141.sql
	Dim verArr
	Dim verArr2
	verArr  = Split(vstrFileName,".")
	verArr2 =  Split(verArr(1),"_")
	
    GetDBVersionFromFileName = verArr2(2)+"."+verArr(2)+"."+verArr(3)
End Function

Public Function GetDatabaseVersion(ByVal vstrPath)

	Dim strResult, strVersion
	Dim fso
	Dim objFolder, verFile, tmpVerDB

	verFile = ""
	Set fso       = vbld_FSO
	Set objFolder = fso.GetFolder(vstrPath)
	For Each objFile In objFolder.Files
	  If UCase(Right(objFile.Name, 3)) = "SQL" Then
	        tmpVerDB = GetDBVersionFromFileName(objFile.Name)
	  		If verFile < tmpVerDB Then
	  			  verFile = tmpVerDB
	  	    End If		  
	  End If 
	Next	
	GetDatabaseVersion = verFile
End Function
