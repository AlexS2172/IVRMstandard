		<step action='Run Script'>
			<Language>VBScript</Language>
			<Script><![CDATA[Dim CC 'As New ClearCase.Application
Set CC = CreateObject("ClearCase.Application")
' Set a view context for the version comparison
'ChDir "C:\Development\EG1_ETS_ASP_Production_BugFix"
'Dim chd As ChDir.CChangeDir
'Set chd = CreateObject("ChDir.ChangeDir")
'chd.ChangeTo "C:\Development\EG1_ETS_ASP_Production_BugFix"
' Get the project VOB with tag \projects
Dim PVOB 'As CCProjectVOB
Set PVOB = CC.ProjectVOB("\FTDepartment")
'Dim pVies 'As CCViews
Dim pStream 'As ClearCase.CCStream
'Set pVies = CC.Views
Dim S
Dim basel 'As CCBaseline
Dim compBas 'As CCBaselines

S = "<table class=MsoTableGrid border=1 cellspacing=0 cellpadding=0 style='margin-left:8.6pt;border-collapse:collapse;border:none;mso-border-alt: solid windowtext .5pt;mso-yfti-tbllook:480;mso-padding-alt:0in 5.4pt 0in 5.4pt; mso-border-insideh:.5pt solid windowtext;mso-border-insidev:.5pt solid windowtext'>"



Set pStream = PVOB.Stream("IvRm_Integration_2.3")
For Each basel In pStream.LatestBaselines
    Set compBas = pStream.Baselines(basel.Component)
    If compBas.Count >= 2 Then
    
         S = S + "<tr style='mso-yfti-irow:0;mso-yfti-firstrow:yes'><td width=636 colspan=2 valign=top style='width:477.0pt;border:solid windowtext 1.0pt;mso-border-alt:solid windowtext .5pt;background:#CCFFFF;padding:0in 5.4pt 0in 5.4pt'><p class=MsoNormal style='mso-margin-top-alt:auto;mso-margin-bottom-alt:auto'><b style='mso-bidi-font-weight:normal'><span style='color:#003572'><o:p>" + basel.Component.Name + "</o:p></span></b></p></td></tr>"
        
        Dim Baseline1 'As CCBaseline
        Dim Baseline2 'As CCBaseline
        Dim bHasChanges
        
        bHasChanges = False
        Set Baseline2 = compBas.Item(compBas.Count)
        Set Baseline1 = compBas.Item(compBas.Count - 1)
        'Set Baseline1 = compBas.Item(1)

        Dim BaselineComp 'As CCBaselineComparison
        Set BaselineComp = CC.CreateBaselineComparison
    
        BaselineComp.BaselineOne = Baseline1
        BaselineComp.BaselineTwo = Baseline2
        BaselineComp.Compare
        
        Dim Activities 'As CCActivities
        Dim Activity   'As CCActivity

        Set Activities = BaselineComp.ActivitiesInTwoButNotOne

        For Each Activity In Activities
           If InStr(1, Activity.Headline, "build", vbTextCompare) = 0 And InStr(1, Activity.Headline, "rebase", vbTextCompare) = 0 And InStr(1, Activity.Headline, "deliver", vbTextCompare) = 0 Then
                S = S + "<tr style='mso-yfti-irow:1'><td width=319 valign=top style='width:239.0pt;border:solid windowtext 1.0pt; border-top:none;mso-border-top-alt:solid windowtext .5pt;mso-border-alt:solid windowtext .5pt; padding:0in 5.4pt 0in 5.4pt'><p class=MsoNormal style='mso-margin-top-alt:auto;mso-margin-bottom-alt:auto'><span style='color:#003572'><o:p>" + Activity.Name + "</o:p></span></p></td>"
                S = S + "<td width=317 valign=top style='width:238.0pt;border-top:none;border-left: none;border-bottom:solid windowtext 1.0pt;border-right:solid windowtext 1.0pt; mso-border-top-alt:solid windowtext .5pt;mso-border-left-alt:solid windowtext .5pt; mso-border-alt:solid windowtext .5pt;padding:0in 5.4pt 0in 5.4pt'> <p class=MsoNormal style='mso-margin-top-alt:auto;mso-margin-bottom-alt:auto'><span style='color:#003572'><o:p>" + Activity.Headline + "</o:p></span></p></td></tr>"
                bHasChanges = True
           End If
        Next
        
        Set Activities = BaselineComp.ChangedActivities
        For Each Activity In Activities
           If InStr(1, Activity.Headline, "build", vbTextCompare) = 0 And InStr(1, Activity.Headline, "rebase", vbTextCompare) = 0 And InStr(1, Activity.Headline, "deliver", vbTextCompare) = 0 Then
                S = S + "<tr style='mso-yfti-irow:1'><td width=319 valign=top style='width:239.0pt;border:solid windowtext 1.0pt; border-top:none;mso-border-top-alt:solid windowtext .5pt;mso-border-alt:solid windowtext .5pt; padding:0in 5.4pt 0in 5.4pt'><p class=MsoNormal style='mso-margin-top-alt:auto;mso-margin-bottom-alt:auto'><span style='color:#003572'><o:p>" + Activity.Name + "</o:p></span></p></td>"
                S = S + "<td width=317 valign=top style='width:238.0pt;border-top:none;border-left: none;border-bottom:solid windowtext 1.0pt;border-right:solid windowtext 1.0pt; mso-border-top-alt:solid windowtext .5pt;mso-border-left-alt:solid windowtext .5pt; mso-border-alt:solid windowtext .5pt;padding:0in 5.4pt 0in 5.4pt'> <p class=MsoNormal style='mso-margin-top-alt:auto;mso-margin-bottom-alt:auto'><span style='color:#003572'><o:p>" + Activity.Headline + "</o:p></span></p></td></tr>"
                bHasChanges = True
           End If
        Next
        
        If bHasChanges = False Then
        S = S + "<tr style='mso-yfti-irow:1'><td width=319 valign=top style='width:239.0pt;border:solid windowtext 1.0pt; border-top:none;mso-border-top-alt:solid windowtext .5pt;mso-border-alt:solid windowtext .5pt; padding:0in 5.4pt 0in 5.4pt'></td>"
        S = S + "<td width=317 valign=top style='width:238.0pt;border-top:none;border-left: none;border-bottom:solid windowtext 1.0pt;border-right:solid windowtext 1.0pt; mso-border-top-alt:solid windowtext .5pt;mso-border-left-alt:solid windowtext .5pt; mso-border-alt:solid windowtext .5pt;padding:0in 5.4pt 0in 5.4pt'> </td></tr>"
        End If
    End If
        
Next
S = S + "</table>"

Application.Macros(vbldMacroTemporary).Add "ACTIVITY", S
]]></Script>
